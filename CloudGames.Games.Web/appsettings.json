{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "GamesDb": "Server=(localdb)\\MSSQLLocalDB;Database=CloudGames.Games;Trusted_Connection=True;TrustServerCertificate=True",
    "Storage": "UseDevelopmentStorage=true"
  },
  "Payments": {
    "BaseUrl": "http://localhost:5003"
  },
  "Queues": {
    "Games": "games-events"
  },
  "JwtSettings": {
    "Secret": "your_development_secret_change_me",
    "Issuer": "cloudgames",
    "Audience": "cloudgames"
  }
}
